{
	"compilerOptions": {
	  "module": "CommonJS",
	  "target": "ES2020",
	  "lib": ["ES2020"],
	  "sourceMap": true,
	  "rootDir": "src",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "moduleResolution": "node",
	  "types": ["node"]
	},
	"exclude": ["node_modules", ".vscode-test"]
  }